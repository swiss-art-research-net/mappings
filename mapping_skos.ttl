crm:E21_Person rdf:type rdfs:Class ;
skos:closeMatch gnd:Person ; #closeMatch is been used because the equivalence of the classes is partial. gnd:person includes as subclass also "characters" and "spirits" .
skos:exactMatch gvp:PersonConcept ;
skos:exactMatch wd:Q5 ; #human in wikidata
skos:exactMatch wd:Q154954 ;  #natural person in wikidata
skos:exactMatch wd:Q1415187 . #economic agent in wikidata

crm:E74_Group rdf:type rdfs:Class ;
skos:exactMatch gvp:GroupConcept;
skos:closeMatch wd:Q8436 .

crm:E40_Legal_body rdf:type rdfs:Class ;
skos:exactMatch gnd:CorporateBody ;
skos:exactMatch wd:Q43229 ; #organzation in wd
skos:closeMatch wd:Q155076 . #juridical person in wd

crm:E22_Man-Made_Object rdf:type rdfs:Class ;
skos:closeMatch gnd:Work ;
skos:exactMatch wd:Q223557 ; #physical object. Subclass contains intellectual work
skos:exactMatch wd:Q11828321 ;
skos:exactMatch wd:Q4406616 ; # equivalent to physical object, but no formalisation has been found
skos:closeMatch wd:Q17553950 . # tangible object. subclass is (also) artificial entity, which do imply possibility of mental objects...

crm:E25_Man-Made_Feature rdf:type rdfs:Class ;
skos:closeMatch gnd:ProvenanceCharacteristic . #Provenance features indicate traces left by previous owners (people and institutions) in and on books, manuscripts or other artifacts. These features indicate ownership, read tracks or censorship intentions.

crm:E7_Activity rdf:type rdfs:Class ;
skos:exactMatch gnd:ConferenceOrEvent ;
skos:exactMatch wd:Q1914636 ; #activity in wd
skos:closeMatch wd:Q13200882 ; #act in wd as "process of doing something"
skos:closeMatch wd:Q4026292 ; # action in wd as "something an agent can do or perform"
sko:closeMatch wd:Q3769299 . #behaviour. "internally coordinated responses (actions or inactions) of entities (individuals or groups) to internal or external stimuli". Include human behaviour and rituals.

crm:E5_Event rdf:type rdfs:Class ;
skos:exactMatch wd:Q1656682 ;

crm:E53_Place rdf:type rdfs:Class ;
skos:exactMatch gnd:PlaceOrGeographicName ;
skos:exactMatch wd:Q17334923 ; #location in wd
skos:exactMatch wd:Q82794 ; #geographical region in wd
skos:exactMatch wd:Q2221906 ; #"point or an area on the Earth's surface or elsewhere" 
skos:exactMatch gvp:PhysPlaceConcept ;
skos:exactMatch gvp:AdminPlaceConcept ;
skos:exactMatch gvp:PhysAdminPlaceConcept .

crm:E89_Propositional_Object rdf:type rdfs:Class ;
skos:exactMatch gnd:SubjectHeading ;
skos:closeMatch wd:Q164573 ; #subject in wd
skos:closeMatch wd:Q108163 ; #proposition in wd
skos:exactMatch gvp:Subject ; #subject in gvp

crm:E28_Conceptual_Object rdf:type rdfs:Class ;
skos:exactMatch wd:Q151885 ; #conceptual object in wd
skos:exactMatch wd:Q7184903 ; #abstract object in wd
skos:exactMatch wd:Q2145290 ; #mental representation in wd



